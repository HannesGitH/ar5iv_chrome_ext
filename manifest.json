{
  "name": "arxiv -> ar5iv",
  "description": "Automatically replace arxiv links with ar5iv",
  "version": "1.0.0.1",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "browser_action": {
    "default_icon": "icon128.png"
  },
  "permissions": [
    "webRequest"
  ],
  "icons": {"128": "icon128.png"}
}
